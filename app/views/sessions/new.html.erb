<h1>Sign in</h1>
<form action="<%= session_url %>" method="post">
  <%= send_auth_token %>
  <label>Username
    <input type="string" name="user[username]" value="<%= @user.username %>">
  </label>
  <label>Password
    <input type="string" name="user[password]" value="<%= @user.password %>">
  </label>
  <button>Sign In</button>
</form>
